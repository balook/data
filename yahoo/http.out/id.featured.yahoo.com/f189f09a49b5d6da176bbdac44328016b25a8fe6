$(document).ready(function() {
	if($.cookie('TEXT_SIZE')) {
  		$('body').addClass($.cookie('TEXT_SIZE')); 
 	}
 	$('.fontcontainer a').click(function() {
  		var textSize = $(this).parent().attr('class');

        // if it doesn't move over the menu within 1sec,
        // hide the menu
        $('#language-dropdown').mouseleave(function (e) {
            languageLeaveWait = setTimeout(function () {
                if (overMenu === false) {
                    removeLanguageOverlay(e);
                }
            }, 1000);
        });
    
    
        function removeLanguageOverlay() {

            $('.pure-menu').toggleClass('active'); // the menu
            $('#menuLink').toggleClass('active'); // hamburger icon
            $('.footer').toggleClass('active'); // footer needs to slide too
            $('.mobile-header').toggleClass('active'); // top bar needs to slide too

/* General Script Starts */

/* Global Var */
var pckry; //grid container
var scrolled; //distance scrolled by browser
var desktopnavslim; //distance to change nav to slim
var browserheight;
var contentarea;

function getGlobalVar(){
	scrolled = $(window).scrollTop();
	
	desktopnavslim = $('.homebanner_container').height();
	
    
	if(desktopnavslim == null || desktopnavslim == 0){
		desktopnavslim = 10; //min cut off before change (for when no banner)
	}
	browserheight = $(window).height();
}
function isInArray(value, array) {
  return array.indexOf(value) > -1;
}
function packeryoverwrite(){
    // overwrite Packery methods
    var __resetLayout = Packery.prototype._resetLayout;
    Packery.prototype._resetLayout = function() {
    __resetLayout.call( this );
    // reset packer
    var parentSize = getSize( this.element.parentNode );
    var colW = this.columnWidth + this.gutter;
    this.fitWidth = Math.floor( ( parentSize.innerWidth + this.gutter ) / colW ) * colW;
    this.packer.width = this.fitWidth;
    this.packer.height = Number.POSITIVE_INFINITY;
    this.packer.reset();
    };

    Packery.prototype._getContainerSize = function() {
    // remove empty space from fit width
    var emptyWidth = 0;
    for ( var i=0, len = this.packer.spaces.length; i < len; i++ ) {
        var space = this.packer.spaces[i];
        if ( space.y === 0 && space.height === Number.POSITIVE_INFINITY ) {
            emptyWidth += space.width;
        }
    }
    return {
        width: this.fitWidth - this.gutter - emptyWidth,
        height: this.maxY - this.gutter
    };
    };
    // always resize
    Packery.prototype.needsResizeLayout = function() {
        return true;
    };
}
function packeryint(){
    var container = document.querySelector('.gridcontainer');
    pckry = new Packery( container, {
        itemSelector: '.item',
        stamp: ".lrecstamp",
        gutter: 10
    });
    $(".gridcontainer").css('opacity', 1);
}
function indexNavbar(){
	if (scrolled>desktopnavslim){
		$('.main_nav').addClass('smallnav');
	}
	else{
		$('.main_nav').removeClass('smallnav');
	}
}
function menu(){
    $(".hamburger_container").click(function() {
        if($(".hamburger_container").hasClass("menuopen")){
            $(".hamburger_container").removeClass("menuopen");
            $(".menu_container").removeClass("menuopen");
            $(".main_nav").removeClass("menuopen");
        }
        else{
            $(".hamburger_container").addClass("menuopen");
            $(".menu_container").addClass("menuopen");
            $(".main_nav").addClass("menuopen");
        }
    });
}
function imagesizeReduce(originalurl){
    //Reducing loaded img size start to 250
    var imgcontainer = document.createElement('a');
    imgcontainer.href = originalurl;
    var imgdomain = imgcontainer.host.slice(-10);
    if(imgdomain=="tumblr.com"){
        var img_trimmed = originalurl.slice(0, originalurl.lastIndexOf("_")-originalurl.length);
        var img_type = originalurl.slice(-4);
        return img_trimmed+"_250"+img_type;
    }
    else{
        return originalurl;
    }
    //Reducing loaded img size end
}
function preparegrid(limit){
    var pushcount = 0;
    var tag_clean;
    $('.preparegrid').each(function(){
        //Pull category
        var tagcontainer = $(this).find('.item_tagraw');
        var tags = tagcontainer.html();
        tag_clean = tags.slice(0, -1).split(",");
        tagcontainer.empty();
        if(tag_clean.length > 1){
            //second space ID as cat for CM theme
            $(this).find('.item_cat').text(tag_clean[1]);
        }
        
        //Pull image and title
        if (limit != null && pushcount >= limit ){
            $(this).remove();
        }
        else{
        var datalocation = $(this).find('.item_raw');
        
        datalocation.each(function(){
            //prepare data
            var dataraw = eval($(this).html());
            var imglink = "";
            var title = "";
            
            if($(this).hasClass("grabimage")){
                var rex = /<img.*?src=['"](.*?)['"]/;
                var imgarray = rex.exec(dataraw);
                if(imgarray != null){
                    imglink = imgarray[1];
                    $(this).siblings('.item_image').empty().prepend('<img src="' + imglink + '"/>');
                }
            }
            if($(this).hasClass("grabtitle")){
                var rex_header = /\<.*?h2.*?\>(.*?)\<.*?\/h2.*?\>/;
                var titlearray = rex_header.exec(dataraw);
                if(titlearray != null){
                    title = titlearray[1];
                    $(this).siblings('.item_desc').append('<h3>'+title+'</h3>');
                }
                else{
                    var rex_para = /\<.*?p.*?\>(.*?)\<.*?\/p.*?\>/;
                    var titlearray = rex_para.exec(dataraw);
                    if(titlearray != null){
                        title = titlearray[1];
                        $(this).siblings('.item_desc').append('<h3>'+title+'</h3>');
                    }
                }
            }
            
            /* Process amplify article start */
            if(isInArray("amplify", tag_clean) || isInArray("external", tag_clean)){
                var rexlink = /<a\s+(?:[^>]*?\s+)?href="([^"]*)"/;
                var articlelink = rexlink.exec(dataraw);
                if(articlelink != null){
                    //important to remove excess to make link work
                    articlelink[1] = articlelink[1].replace("amp;","");
                    $(this).parent('.item_link').attr("href", articlelink[1]);
                }
            } 
            /* Process amplify article end */
            
            $(this).remove();
            
        });
        pushcount++;
        $(this).removeClass("preparegrid");
        }
    });
}

function getAdv(){
    var s = document.createElement("script");
    s.type = "text/javascript";

    var screensize = window.outerWidth;
    if(screensize<window.innerWidth){
        screensize = window.innerWidth;
    }
    if(articletags[0] != null){
        var basecall = "https://fc.yahoo.com/sdarla/php/client.php?l=";
        var mastAdvert = "MAST{dest:MASTAD;asz:970x250}";
        var lrecOneAvert = "LREC{dest:lrec_one;asz:300x250}";
        var lrecThreeAvert = "LREC3{dest:lrec_m;asz:300x250}";
        if(screensize>990){
            s.src = basecall+lrecOneAvert+","+mastAdvert+"&f="+articletags[0];
        }
        else if(screensize<=990 && screensize>770){
            s.src = basecall+lrecOneAvert+"&f="+articletags[0];
        }
        else{
            s.src = basecall+lrecThreeAvert+"&f="+articletags[0];
        }
    }
    document.getElementById("advertout").innerHTML = "";
    document.getElementById("advertout").appendChild(s);
}

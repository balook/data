function indexbanner(){
    $('.banner_frame').flickity({
        autoPlay: 7000,
        wrapAround: true,
        pauseAutoPlayOnHover: false,
        prevNextButtons: true,
        cellSelector: '.bannercontent',
        selectedAttraction: 0.05,
		friction: 0.5
    });

    var desktopbanner = $('.banner_frame').data('flickity');
    $('.countertotal').text(desktopbanner.cells.length);

    $('.homebanner_container').addClass("bannercount"+desktopbanner.cells.length);

	var bgurl = $('.is-selected').css('background-image');
	var bgurlclean = bgurl.replace('url(','').replace(')','').replace(/\"/gi, "");
	$('.background_layer').css("background-image", bgurl);
	$('.background_layer').find('img').attr("src", bgurlclean);
 
    $('.banner_frame').on( 'settle.flickity', function( event, index ) {
    	var bgurl = $('.is-selected').css('background-image');
    	var bgurlclean = bgurl.replace('url(','').replace(')','').replace(/\"/gi, "");
    	$('.background_layer').css("background-image", bgurl);
    	$('.background_layer').find('img').attr("src", bgurlclean);

    });
}

